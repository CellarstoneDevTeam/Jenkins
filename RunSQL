node
{
    def Target= "${params.Target}"
    stage('Restore QCommDB')
    { 
    
        
          def MSBuildPath = env.MSBuildPath
          def QCVSSWorkFolder = env.QCVSSWorkFolder
          echo '''
          "'''+ MSBuildPath +  '''" ''' +  QCVSSWorkFolder.replace("\\\\", "\\") + "\\BuildScript" +'\\BuildWeb.msbuild'  +  ''' /t:''' + Target  
    }
}
